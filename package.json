    "debug": "~2.6.9",
    "express": "~4.16.1",
    "json-middleware": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.9",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "urlencode": "^1.1.0"
